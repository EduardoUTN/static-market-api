{
  "clean_urls": true,
  "https_only": true,
  "error_page": "errors/error.json",
  "routes": {
    "/*.html": "index.html",
    "/products/": "/products.json"
  },
  "headers": {
    "/**": {
      "Access-Control-Allow-Origin": "*"
    },
    "**.json": {
      "Content-Type": "application/json"
    }
  }
}
